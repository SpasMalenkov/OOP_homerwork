package test3;

public class Client {
	
	private String name;
	private String gsm;
	private long number;
	private String address;
	private CityName city;
	
	public Client(String name, String gsm, long number, String address, CityName city){
		this.name = name;
		this.gsm = gsm;
		this.number = number;
		this.address = address;
		this.city = city;
	}
	
}
