package human_exercises;

public class Student extends Human{
	
	private double mark;
	public Student(String name, String surname, double mark) {
		super(name, surname);
		setMark(mark);
	}
	
	public double getMark() {
		return mark;
	}
	
	public void setMark(double mark) {
		
		if(mark < 2){
			System.out.println("Unvalid entry");
		}else{
			this.mark = mark;
		}
		
	}
	
	
}
