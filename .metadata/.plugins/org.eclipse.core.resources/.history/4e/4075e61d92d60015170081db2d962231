package warehouse;

public class Fruit extends Product implements Runnable{
	
	private String name;
	private int quantity;
	
	Fruit(String name, int quantity){
		this.name = name;
		this.quantity = quantity;
	}
	
	public int getTimeToProduce() {
		return 100;
	}
	public String getProductName(){
		return this.name;
	}
}
