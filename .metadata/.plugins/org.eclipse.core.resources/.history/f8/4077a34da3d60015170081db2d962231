package warehouse;

public class FruitException extends Exception {
	
	private static final long serialVersionUID = 1L;

	public FruitException() {
		// TODO Auto-generated constructor stub
	}

	public FruitException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public FruitException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public FruitException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public FruitException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}
}
